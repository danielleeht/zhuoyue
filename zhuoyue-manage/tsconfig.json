{
  "buildOnSave": false,
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "removeComments": false,
    "noEmitHelpers": false,
    "noImplicitAny": false,
    "lib": [
      "es5",
      "dom"
    ],
    "strictNullChecks": false,
    "types": [
      "core-js",
      "hammerjs",
      "jasmine",
      "node",
      "selenium-webdriver",
      "protractor",
      "source-map",
      "uglify-js",
      "webpack"
    ]
  },
  "awesomeTypescriptLoaderOptions": {
    "resolveGlobs": true,
    "forkChecker": true
  },
  "exclude": [
    "node_modules"
  ]
}
